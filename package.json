{
  "name": "payment-gateway",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,css,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "eslint": "^9.19.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "turbo": "^2.2.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22.11.0"
  },
  "dependencies": {
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
